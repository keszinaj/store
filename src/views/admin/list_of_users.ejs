<%- include('./head_v1') %>
  <main class="mx-5 px-5">
    <div class="p-5 mx-5">
      <table class="table bg-white">
        <thead class="bg-light">
          <tr>
            <th>User ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th>City</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>


          <% users.forEach(user=> { %>
            <tr>
                <%- include('./partial/table_row', {content: user.ID}) %>
                <%- include('./partial/table_row', {content: user.Name}) %>
                <%- include('./partial/table_row', {content: user.Surname}) %>
                <%- include('./partial/table_row', {content: user.Email}) %>
                <%- include('./partial/table_row', {content: user.Phone_number}) %>
                <%- include('./partial/table_row', {content: user.City}) %>
                <% let ref='/admin/user/' + user.ID %>
                <td>
                  <a href="<%=ref %>" class="btn btn-sm btn-outline-primary px-4">More</a>
                </td>
            </tr>
            <% }) %>

        </tbody>
      </table>
    </div>

    <div class="d-flex justify-content-center px-2 m-2">
      <a href="#" class="btn btn-primary disabled mx-5 px-5">Prev</a>
      <a href="#" class="btn btn-primary disabled mx-5 px-5">Next</a>

    </div>
  </main>
  <%- include('./footer') %>